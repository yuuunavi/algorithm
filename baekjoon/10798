#include <iostream>
using namespace std;

char a[15][15];

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	for (int i = 0; i < 5; i++) {
		cin >> a[i];
	}

	for (int i = 0; i < 15; i++) {
		for (int j = 0; j < 5; j++) {
			if (a[j][i] != NULL)
				cout << a[j][i];
		}
	}

	return 0;
}
