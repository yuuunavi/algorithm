#include <iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int a, b, c;
    cin >> a >> b;
    if(a > b) {
        c = a - b;
        c = b - c;
    }
    else {
        c = b - a;
        c += b;
    }
    cout << c << endl;
    return 0;
}
