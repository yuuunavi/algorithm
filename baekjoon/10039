#include <iostream>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int a[5];
    cin >> a[0] >> a[1] >> a[2] >> a[3] >> a[4];
    int result = 0;

    for(int i=0; i<5; i++){
        if(a[i] < 40) a[i] = 40;
        result += a[i];
    }
    cout << result/5 << endl;
    return 0;
}
