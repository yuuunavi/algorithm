#include <iostream>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n, a, b, c;

    cin >> n;
    if(n%10 != 0){
        cout << -1 << endl;
        return 0;
    }
    a = n/300;
    n %= 300;

    b = n/60;
    n %= 60;

    c = n/10;

    cout << a << endl << b << endl << c << endl;
    return 0;
}
