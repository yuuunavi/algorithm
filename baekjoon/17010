#include <iostream>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int t, l;
    char x;
    cin >> t;
    
    for(int i=0; i<t; i++){
        cin >> l >> x;
        for(int j=0; j<l; j++){
            cout << x;
        }
        cout << endl;
    }
    return 0;
}
