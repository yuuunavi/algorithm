#include <iostream>
#include <algorithm>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int t, ans;
    int a[10];
    cin >> t;
    for(int i=0; i<t; i++){
        for(int i=0; i<10; i++) cin >> a[i];
        sort(a, a+10);
        cout << a[7] << endl;
    }
    return 0;
}
