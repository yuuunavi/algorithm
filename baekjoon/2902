#include <iostream>
#include <string>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    string s;
    cin >> s;

    cout << s[0];
    for(int i=0; i<s.size(); i++){
        if(s[i] == '-') {
            cout << s[i+1];
        }
    } cout << endl;
    return 0;
}
