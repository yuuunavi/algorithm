#include <iostream>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n, score;
    string name;
    cin >> n;
    for(int i=0; i<n; i++){
        cin >> name >> score;
        if(score > 96) cout << name << " A+" << endl;
        else if(score > 89) cout << name << " A" << endl;
        else if(score > 86) cout << name << " B+" << endl;
        else if(score > 79) cout << name << " B" << endl;
        else if(score > 76) cout << name << " C+" << endl;
        else if(score > 69) cout << name << " C" << endl;
        else if(score > 66) cout << name << " D+" << endl;
        else if(score > 59) cout << name << " D" << endl;
        else  cout << name << " F" << endl;
    }
    return 0;
}
