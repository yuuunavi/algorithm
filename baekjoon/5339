#include <iostream>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    cout << "     /~\\" << endl;
    cout << "    ( oo|" << endl;
    cout << "    _\\=/_" << endl;
    cout << "   /  _  \\" << endl;
    cout << "  //|/.\\|\\\\" << endl;
    cout << " ||  \\ /  ||" << endl;
    cout << "============" << endl;
    cout << "|          |" << endl;
    cout << "|          |" << endl;
    cout << "|          |" << endl;

    return 0;
}
