#include <iostream>
#include <string>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    string s;
    int ans = 1;
    cin >> s;
    for(int i=0; i<s.size(); i++){
        if(s[i] == ',') ans++;
    }
    cout << ans << endl;
    return 0;
}
