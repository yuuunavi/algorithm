#include <iostream>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n, t, ans;
    cin >> t;
    for(int i=0; i<t; i++){
        ans = 0;
        cin >> n;
        for(int j=1; j<=n; j++){
            if(j % 2 == 1) ans += j;
        }
        cout << ans << endl;
    }
    return 0;
}
