#include <iostream>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int t, n;
    string s;
    cin >> t;
    while(t--){
        cin >> n >> s;
        for(int i=0; i<s.length();i++){
            if(i != n-1) cout << s[i];
        }
        cout << endl;
    }
    return 0;
}
