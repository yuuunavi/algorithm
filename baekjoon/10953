#include <iostream>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n, a, b, ans;
    char p;
    cin >> n;
    for(int i=0; i<n; i++){
        cin >> a >> p >> b;
        ans = a + b;
        cout << ans << endl;
    }
    return 0;
}
