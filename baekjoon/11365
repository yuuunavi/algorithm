#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    string str;
    while(1){
        getline(cin, str);
        if(str == "END"){
            break;
        }
        reverse(str.begin(), str.end());
        cout << str << endl;
    }
    return 0;
}
