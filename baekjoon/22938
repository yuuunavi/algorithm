#include <iostream>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    long x1, x2, r1, y1, y2, r2, d;
    cin >> x1 >> x2 >> r1 >> y1 >> y2 >> r2;

    d = ((x1 - y1) * (x1 - y1)) + ((x2 - y2) * (x2 - y2));

    if(d < (r1+r2)*(r1+r2)) cout << "YES" << endl;
    else cout << "NO" << endl;
    return 0;
}
