#include <iostream>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int t, n, r, tmp;
    cin >> t;
    for(int i=0; i<t; i++){
        r = 0;
        cin >> n;
        for(int i=0; i<n; i++){
            cin >> tmp;
            r += tmp;
        }
        cout << r << endl;
    }
    return 0;
}
