#include <iostream>
using namespace std;
int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n, ans = 0;
    cin >> n;

    for(int i=0; i<=n; i++){
        for(int j=0; j<=i; j++){
            ans += i + j;
        }
    }
    cout << ans << endl;
    return 0;
}
