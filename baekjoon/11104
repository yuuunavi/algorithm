#include <iostream>
#include <math.h>
#include <string>
using namespace std;

int main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);

    int n;
    string num;
    cin >> n;

    for(int i=0; i<n; i++){
        cin >> num;
        cout << stoi(num, 0, 2) << endl;
    }
    return 0;
}
